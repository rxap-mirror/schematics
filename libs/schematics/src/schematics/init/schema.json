{
  "$schema": "http://json-schema.org/schema",
  "id": "init",
  "type": "object",
  "description": "",
  "properties": {
    "zeplin": {
      "type": "boolean",
      "description": "Add zeplin support",
      "default": false
    },
    "storybook": {
      "type": "boolean",
      "description": "Add storybook support",
      "default": false
    },
    "zeplinProject": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "A list of zeplin project ids",
      "default": [],
      "alias": "zeplin-project"
    },
    "zeplinStyleguide": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "A list of zeplin styleguide ids",
      "default": [],
      "alias": "zeplin-styleguide"
    },
    "zeplinConnectUrl": {
      "type": "string",
      "description": "The zeplin connect url of the storybook",
      "alias": "zeplin-connect-url"
    }
  },
  "required": []
}
