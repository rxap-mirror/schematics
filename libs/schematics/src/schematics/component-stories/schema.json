{
  "$schema": "http://json-schema.org/schema",
  "id": "component-stories",
  "type": "object",
  "description": "",
  "properties": {
    "name": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "path": {
      "type": "string",
      "format": "path",
      "description": "",
      "visible": false
    },
    "project": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "projectName"
      }
    },
    "prefix": {
      "type": "string",
      "description": "The prefix to apply to the generated component selector.",
      "alias": "p",
      "oneOf": [
        {
          "maxLength": 0
        },
        {
          "minLength": 1,
          "format": "html-selector"
        }
      ]
    }
  },
  "required": []
}
